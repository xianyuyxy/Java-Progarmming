import java.util.Scanner;

public class classes {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("sum from 1 to 10 is "+sum(1,10));
    }
    public static int sum(int a,int b){
        int s=0;
        for(int i=a;i<=b;i++){
            s+=i;
        }
        return s;
    }
}

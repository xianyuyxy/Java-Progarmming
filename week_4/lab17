import java.util.Scanner;

public class dcv {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("enter the x:");
        int x=input.nextInt();
        System.out.println("enter a:");
        int a=input.nextInt();
        switch (a){
            case 1:x+=5;break;
            case 2:x+=10;break;
            case 3:x+=16;break;
            case 4:x+=34;break;
        }
        System.out.println("the new x is "+x);
    }
}
